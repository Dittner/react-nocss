{
  "name": "react-nocss",
  "description": "CSS-in-JSX-lib",
  "version": "1.0.6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dittner/react-nocss"
  },
  "keywords": [
    "react",
    "CSS-in-JSX",
    "NoCSS"
  ],
  "author": "Alexander Dittner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dittner/react-nocss/issues"
  },
  "homepage": "https://github.com/Dittner/react-nocss",
  "private": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "index.ts",
    "LICENSE.md",
    "README.md"
  ],
  "peerDependencies": {
    "@types/node": "^16.18.95",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.11.1",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "prismjs": "^1.29.0",
    "react-markdown": "^8.0.7"
  },
  "scripts": {
    "demo": "PUBLIC_URL=/ react-scripts start",
    "test": "jest",
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "ts-loader": "^9.5.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "ts-jest": "^29.1.1"
  }
}
